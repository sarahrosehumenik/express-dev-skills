<!DOCTYPE html>
<html>
  <head>
    <title>express skills</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <body>
      <h1>Skillz yo</h1>
      <ul>
        <% skills.forEach(function(t) { %>
          <li>
            <a href="/skills/<%= t.id %>"><%= t.skill %></a>
              - 
            <%= t.done ? 'done' : 'not done' %>
          </li>
        <% }); %>
      </ul>
    </body>
  </body>
</html>